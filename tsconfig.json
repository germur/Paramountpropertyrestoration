{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "types": ["astro/client", "vite/client"],
    "skipLibCheck": true
  },
  "include": [
    "src",
    "astro.config.*",
    "env.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules",
    ".astro"
  ]
}